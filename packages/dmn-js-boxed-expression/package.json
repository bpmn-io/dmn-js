    "directory": "packages/dmn-js-boxed-expression"
  },
  "license": "SEE LICENSE IN LICENSE",
  "keywords": [
    "dmn",
    "dmn-js",
    "dmn-js-view",
    "boxed expression"
  ],
  "devDependencies": {
    "axe-core": "^4.11.1",
    "cross-env": "^10.0.0",
    "dmn-font": "^0.6.2",
    "inferno-test-utils": "~5.6.2"
  },
  "dependencies": {
    "@bpmn-io/dmn-variable-resolver": "^0.7.0",
    "diagram-js": "^15.6.0",
    "dmn-js-shared": "^17.5.0",
    "inferno": "~5.6.3",
    "min-dash": "^5.0.0",
    "min-dom": "^5.1.2",
    "table-js": "^9.4.0"
