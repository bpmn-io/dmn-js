{
  "name": "dmn-js-boxed-expression",
  "description": "A boxed expression view for dmn-js",
  "version": "17.5.0",
  "files": [
    "assets",
    "lib"
  ],
  "scripts": {
    "test": "karma start",
    "build": "del-cli lib && babel -s --quiet -d lib src",
