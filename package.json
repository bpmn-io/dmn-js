{
  "name": "dmn-js-build",
  "version": "1.1.0",
  "scripts": {
    "all": "run-s lint test",
    "lint": "eslint packages",
    "distro": "lerna run prepublishOnly",
    "dev": "lerna run dev --stream --scope",
    "clean": "lerna clean --yes",
    "postinstall": "lerna bootstrap --hoist",
